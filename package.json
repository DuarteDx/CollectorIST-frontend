{
  "name": "scholar-frontend",
  "version": "1.0.0",
  "description": "The FenixEdu Scholar Frontend",
  "author": "DSI <dsi@tecnico.ulisboa.pt>",
  "private": true,
  "scripts": {
    "mock": "MOCK=true webpack-dev-server --hot --inline --config build/webpack.conf.dev.js",
    "dev": "webpack-dev-server --hot --inline --config build/webpack.conf.dev.js",
    "start": "npm run dev",
    "dist": "webpack --config build/webpack.conf.prod.js",
    "unit": "cross-env BABEL_ENV=test karma start test/unit/karma.conf.js --single-run",
    "e2e": "node test/e2e/runner.js",
    "test": "npm run unit && npm run e2e",
    "lint": "eslint --ext .js,.vue src --fix",
    "analysis": "webpack --mode=production --display-reasons --json --config build/webpack.conf.prod.js > build/analysis.json",
    "prodServer": "http-server dist/"
  },
  "dependencies": {
    "axios": "0.18.0",
